:{}},{"port":[8],"name":"TouchLED8","customName":false,"deviceType":"TouchLED","deviceClass":"touchled","setting":{}}],"slot":1,"platform":"IQ","sdkVersion":"20230818.11.00.00","appVersion":"4.0.7","minVersion":"3.0.0","fileFormat":"2.0.0","targetBrainGen":"Second","v5SoundsEnabled":false,"downloadLanguage":"python","cppStatus":"true","cpp":"// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START IQ MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END IQ MACROS\n\n\n// Robot configuration code.\ninertial BrainInertial = inertial();\nmotor Left = motor(PORT1, true);\nmotor Right = motor(PORT2, false);\nmotor IntakeMotorA = motor(PORT3, true);\nmotor IntakeMotorB = motor(PORT4, false);\nmotor_group Intake = motor_group(IntakeMotorA, IntakeMotorB);\n\ntouchled TouchLED10 = touchled(PORT10);\ntouchled TouchLED8 = touchled(PORT8);\n\n\n// generating and setting random seed\nvoid initializeRandomSeed(){\n  wait(100,msec);\n  double xAxis = BrainInertial.acceleration(xaxis) * 1000;\n  double yAxis = BrainInertial.acceleration(yaxis) * 1000;\n  double zAxis = BrainInertial.acceleration(zaxis) * 1000;\n  // Combine these values into a single integer\n  int seed = int(\n    xAxis + yAxis + zAxis\n  );\n  // Set the seed\n  srand(seed); \n}\n\n\n\nvoid vexcodeInit() {\n\n  // Initializing random seed.\n  initializeRandomSeed(); \n}\n\n\n// Generated code.\n\n// Include the IQ Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\n// User defined function\nvoid myblockfunction_Move_Time_Target_Speed_Pi(double myblockfunction_Move_Time_Target_Speed_Pi__Time, double myblockfunction_Move_Time_Target_Speed_Pi__Target, double myblockfunction_Move_Time_Target_Speed_Pi__Speed, double myblockfunction_Move_Time_Target_Speed_Pi__Pi);\n// User defined function\nvoid myblockfunction_Turn_Tar_Pd(double myblockfunction_Turn_Tar_Pd__Tar, double myblockfunction_Turn_Tar_Pd__Pd);\n\nfloat myVariable, target, error, rid, state;\n\nevent message1 = event();\nevent Rd = event();\nevent RollerRotation = event();\nevent Downgreen = event();\nevent UpLift = event();\nevent DownLift = event();\nevent PurpleLift = event();\n\n// User defined function\nvoid myblockfunction_Move_Time_Target_Speed_Pi(double myblockfunction_Move_Time_Target_Speed_Pi__Time, double myblockfunction_Move_Time_Target_Speed_Pi__Target, double myblockfunction_Move_Time_Target_Speed_Pi__Speed, double myblockfunction_Move_Time_Target_Speed_Pi__Pi) {\n  Brain.Timer.reset();\n  target = myblockfunction_Move_Time_Target_Speed_Pi__Target;\n  Left.spin(forward);\n  Right.spin(forward);\n  while ((Brain.Timer.value() < myblockfunction_Move_Time_Target_Speed_Pi__Time)) {\n    error = target - rid;\n    Left.setVelocity((myblockfunction_Move_Time_Target_Speed_Pi__Speed + error * myblockfunction_Move_Time_Target_Speed_Pi__Pi), percent);\n    Right.setVelocity((myblockfunction_Move_Time_Target_Speed_Pi__Speed - error * myblockfunction_Move_Time_Target_Speed_Pi__Pi), percent);\n  wait(20, msec);\n  }\n  Left.stop();\n  Right.stop();\n}\n\n// User defined function\nvoid myblockfunction_Turn_Tar_Pd(double myblockfunction_Turn_Tar_Pd__Tar, double myblockfunction_Turn_Tar_Pd__Pd) {\n  target = myblockfunction_Turn_Tar_Pd__Tar;\n  state = 1.0;\n  Left.spin(forward);\n  Right.spin(forward);\n  while ((state == 1.0)) {\n    error = (target - rid) * myblockfunction_Turn_Tar_Pd__Pd;\n    if (target > rid + 2.0) {\n      Left.setVelocity((2.0 + error), percent);\n      Right.setVelocity((-2.0 - error), percent);\n    } else if (target < rid - 2.0) {\n      Left.setVelocity((-2.0 + error), percent);\n      Right.setVelocity((2.0 - error), percent);\n    } else {\n      state = 0.0;\n      Left.setVelocity(0.0, percent);\n      Right.setVelocity(0.0, percent);\n    }\n  wait(20, msec);\n  }\n  Left.stop();\n  Right.stop();\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  TouchLED10.setColor(red);\n  TouchLED8.setColor(red);\n  BrainInertial.calibrate();\n  while (BrainInertial.isCalibrating()) { task::sleep(50); }\n  wait(0.1, seconds);\n  Rd.broadcast();\n  Left.setStopping(brake);\n  Right.setStopping(brake);\n  Left.setMaxTorque(100.0, percent);\n  Right.setMaxTorque(100.0, percent);\n  TouchLED10.setColor(green);\n  TouchLED10.setBrightness(100);\n  TouchLED8.setBrightness(100);\n  waitUntil(TouchLED10.pressing());\n  TouchLED10.setColor(white);\n  myblockfunction_Move_Time_Target_Speed_Pi(3.0, 0.0, 80.0, 1.0);\n  myblockfunction_Turn_Tar_Pd(180.0, 0.0);\n  return 0;\n}\n\n// \"when TouchLED8 pressed\" hat block\nvoid onevent_TouchLED8_pressed_0() {\n  Brain.programStop();\n}\n\n// \"when I receive Rd\" hat block\nvoid onevent_Rd_0() {\n  while (true) {\n    rid = BrainInertial.rotation();\n  wait(20, msec);\n  }\n}\n\n\nint main() {\n  // Initializing Robot Configuration. DO NOT REMOVE!\n  vexcodeInit();\n\n  // register event handlers\n  TouchLED8.pressed(onevent_TouchLED8_pressed_0);\n  Rd(onevent_Rd_0);\n\n  wait(15, msec);\n  whenStarted1();\n}","target":"Physical"}